{
    "applicationId": "br.com.thiagosv.kumite",
    "version": "1.0.0",
    "defaultMode": "window",
    "port": 0,
    "documentRoot": "/resources/",
    "url": "/",
    "enableServer": true,
    "enableNativeAPI": true,
    "enableExtensions": true,
    "tokenSecurity": "one-time",
    "logging": {
        "enabled": true,
        "writeToLogFile": true
    },
    "nativeAllowList": [
        "app.*",
        "os.*",
        "debug.log"
    ],
    "modes": {
        "window": {
            "title": "Kumite",
            "width": 1200,
            "height": 800,
            "minWidth": 400,
            "minHeight": 300,
            "resizable": true,
            "exitProcessOnClose": true,
            "icon": "/resources/icons/appIcon.png"
        }
    },
    "cli": {
        "binaryName": "kumite",
        "resourcesPath": "/resources/",
        "extensionsPath": "/extensions/",
        "clientLibrary": "/resources/js/neutralino.js",
        "binaryVersion": "4.14.1",
        "clientVersion": "3.12.0",
        "binaries": {
            "windows": {
                "x64": "kumite-win_x64.exe"
            }
        }
    }
}